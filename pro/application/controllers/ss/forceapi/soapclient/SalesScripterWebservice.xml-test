<definitions xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://soap.sforce.com/schemas/class/sscripter/SalesScripterWebservice" targetNamespace="http://soap.sforce.com/schemas/class/sscripter/SalesScripterWebservice">
